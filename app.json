{
  "expo": {
    "name": "shuttlemate",
    "slug": "shuttlemate",
     "scheme": "shuttlemate",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
     
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/logo.png",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
      "foregroundImage": "./assets/logo.png", 
        "backgroundColor": "#ffffff"
        
      },
      "permissions": ["INTERNET"],
      "package": "com.anonymous.shuttlemate",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/SC.jpg"
    },
    "plugins": [
      "expo-router",
     [
      "@stripe/stripe-react-native",
      {
          "publishableKey":"pk_test_51QJef7RwP0CS6vlpJKa4aIbmfRJdUMqt8K4Lm1dBEM63cbBBvBgHLbpblPVZND1G7iNtTVhNEqCqk1YcFVgWFlqL0084kYVZX8"
      }
     ],
      "expo-font",
      "expo-video",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ca2c1e22-c124-4eb8-938f-f10bfe9730b6"
      }
    }
  }
}
